{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "no-trailing-spaces": "off",
        "indent": "off",
        "no-multiple-empty-lines": "off",
        "eol-last": ["error", "never"],
        "quotes": "off",
        "padded-blocks": "off",
        "object-curly-newline": "off",
        "max-len":"off"
    }
}
